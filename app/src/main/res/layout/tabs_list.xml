<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginStart="35dp">

    <TextView
        android:id="@+id/total_list"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:text="1 Total Item List"
        android:textSize="25sp"
        android:textColor="@android:color/black"
        android:layout_marginBottom="6dp"/>

    <TextView
        android:textColor="@android:color/black"
        android:layout_below="@+id/total_list"
        android:id="@+id/end_list"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:text="2 End item list"
        android:textSize="25sp"
        android:layout_marginBottom="6dp"/>

    <TextView
        android:textColor="@android:color/black"
        android:layout_below="@+id/end_list"
        android:id="@+id/wire_list"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:text="3 Wire List"
        android:textSize="25sp"
        android:layout_marginBottom="6dp"/>
    <TextView
        android:textColor="@android:color/black"
        android:layout_below="@+id/wire_list"
        android:id="@+id/image2dIV"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:text="4  2D Drawing"
        android:textSize="25sp"
        android:layout_marginBottom="6dp"/>

    <TextView
    android:textColor="@android:color/black"
    android:layout_below="@+id/image2dIV"
    android:id="@+id/connectorimgIV"
    android:layout_width="match_parent"
    android:layout_height="60dp"
    android:text="5  Connector Images"
    android:textSize="25sp"
    android:layout_marginBottom="6dp"/>




    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_gravity="bottom|end"
        android:layout_marginBottom="20dp"
        android:layout_marginEnd="20dp"
        app:srcCompat="@android:drawable/ic_btn_speak_now" />

</RelativeLayout>