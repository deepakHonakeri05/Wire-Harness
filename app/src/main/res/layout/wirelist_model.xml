        android:layout_margin="5dp"
        tools:text="TextView" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:text="Pin 2 :"
            android:textColor="@android:color/black"
            android:textSize="17sp"
            android:layout_margin="5dp"
            android:layout_height="wrap_content" />
    <TextView
        android:textSize="17dp"
        android:textColor="@android:color/black"
        android:id="@+id/pin2TV"
        android:layout_weight="1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        tools:text="TextView" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:text="Term 2 :"
            android:textColor="@android:color/black"
            android:textSize="17sp"
            android:layout_margin="5dp"
            android:layout_height="wrap_content" />
    <TextView
        android:textColor="@android:color/black"
        android:textSize="17dp"
        android:id="@+id/term2TV"
        android:layout_weight="1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        tools:text="TextView" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:text="Length :"
            android:textColor="@android:color/black"
            android:textSize="17sp"
            android:layout_margin="5dp"
            android:layout_height="wrap_content" />
    <TextView
        android:textSize="17dp"
        android:textColor="@android:color/black"
        android:id="@+id/lengthTV"
        android:layout_weight="1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        tools:text="TextView" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:text="Design :"
            android:textColor="@android:color/black"
            android:textSize="17sp"
            android:layout_margin="5dp"
            android:layout_height="wrap_content" />
    <TextView
        android:textSize="17dp"
        android:textColor="@android:color/black"
        android:id="@+id/designTV"
        android:layout_weight="1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
